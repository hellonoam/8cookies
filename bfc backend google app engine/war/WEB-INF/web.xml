<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ReceiveData</servlet-name>
		<servlet-class>backend.ReceiveData</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SendData</servlet-name>
		<servlet-class>backend.SendData</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>backend.SignUp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteCookiesFromServer</servlet-name>
		<servlet-class>backend.DeleteCookiesFromServer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BetaSignUp</servlet-name>
		<servlet-class>backend.BetaSignUp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VisitedURL</servlet-name>
		<servlet-class>backend.VisitedURL</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FailedToReproduceURL</servlet-name>
		<servlet-class>backend.FailedToReproduceURL</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StrippedURLs</servlet-name>
		<servlet-class>backend.StrippedURLs</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ShowUniqueURL</servlet-name>
		<servlet-class>backend.ShowUniqueURL</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Error</servlet-name>
		<servlet-class>backend.Error</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserStorageDetails</servlet-name>
		<servlet-class>backend.UserStorageDetails</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReceiveData</servlet-name>
		<url-pattern>/ReceiveData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SendData</servlet-name>
		<url-pattern>/SendData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/SignUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteCookiesFromServer</servlet-name>
		<url-pattern>/DeleteCookiesFromServer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BetaSignUp</servlet-name>
		<url-pattern>/BetaSignUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VisitedURL</servlet-name>
		<url-pattern>/VisitedURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FailedToReproduceURL</servlet-name>
		<url-pattern>/FailedToReproduceURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StrippedURLs</servlet-name>
		<url-pattern>/StrippedURLs</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowUniqueURL</servlet-name>
		<url-pattern>/ShowUniqueURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Error</servlet-name>
		<url-pattern>/Error</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserStorageDetails</servlet-name>
		<url-pattern>/UserStorageDetails</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
